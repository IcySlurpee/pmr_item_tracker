{
  "item_map_tracker": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "content": [
            {
              "type": "group",
              "header": "Star Spirits / Partners",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#CC212121",
                "layout": "combined_settings_popup_grid"
              },
              "content": {
                "type": "layout",
                "key": "stars_partners_grid"
              }
            },
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "compact_combined_items_keys_grid"
              }
            },
            {
              "type": "group",
              "header": "Other Items",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "combined_other_items_grid"
              }
            },
            {
              "type": "group",
              "header": "Counters",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "counters_grid"
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "map",
              "dock": "top",
              "maps": [
                "worldmap"
              ]
            },
            {
              "type": "group",
              "header": "Pinned Checks",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal"
              }
            }
          ]
        }
      ]
    }
  },
  "items_only_tracker": {
    "type": "array",
    "orientation": "vertical",
    "h_alignment": "center",
    "content": [
      {
        "type": "layout",
        "key": "stars_partners_grid"
      },
      {
        "type": "layout",
        "key": "compact_combined_items_grid"
      }
    ]
  },
  "capture_item_view": {
    "type": "array",
    "orientation": "vertical",
    "h_alignment": "center",
    "content": [
      {
        "type": "layout",
        "key": "partner_grid"
      },
      {
        "type": "layout",
        "key": "compact_combined_capture_grid"
      }
    ]
  }
}
